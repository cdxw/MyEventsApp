<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="myEventsApp.Search"
             Title="Search">
  <ContentPage.ToolbarItems>

  </ContentPage.ToolbarItems>
  <ContentPage.Content>
    <StackLayout>

      <Label Text="Search By Name" VerticalOptions="Center" HorizontalOptions="Center" />
      <Entry x:Name="nameSearch"></Entry>
      <Button Text="Search" Clicked="OnBackButtonClicked" VerticalOptions="CenterAndExpand" />
    </StackLayout>

  </ContentPage.Content>
</ContentPage>